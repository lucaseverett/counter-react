{
  "name": "counter-react",
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "start": "parcel ./src/index.html --open --no-cache",
    "build": "rimraf dist && parcel build ./src/index.html --public-url ./ --no-cache",
    "format": "prettier --write 'src/**/*.js'",
    "test": "jest --watch --config ./jest.config.json"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@emotion/babel-plugin": "^11.3.0",
    "@parcel/babel-preset-env": "^2.0.0-nightly.645",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "jest": "^26.6.3",
    "parcel": "^2.0.0-nightly.643",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}
